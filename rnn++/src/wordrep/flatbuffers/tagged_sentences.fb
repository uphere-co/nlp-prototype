namespace wordrep.wiki.io;

struct EntityCandidate{
    idx:long;
    uid:long;
    score:double;
}

struct TaggedEntity{
    idx:long;
    len:ulong;
}

table TaggedSentences{
    candidates:[EntityCandidate];
    tagged_entities:[TaggedEntity];
}

file_identifier "wfts";
file_extension "wfts";

root_type TaggedSentences;
